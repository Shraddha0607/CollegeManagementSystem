{
  "openapi": "3.0.1",
  "info": {
    "title": "CollegeApp",
    "version": "1.0"
  },
  "paths": {
    "/api/Applicant": {
      "post": {
        "tags": [
          "Applicant"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ApplicantRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ApplicantRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ApplicantRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ApplicantResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApplicantResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApplicantResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ApplicantRequest": {
        "required": [
          "aadharNo",
          "course",
          "name",
          "session"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "aadharNo": {
            "maxLength": 10,
            "minLength": 1,
            "type": "string"
          },
          "course": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "departmentId": {
            "type": "integer",
            "format": "int32"
          },
          "session": {
            "maximum": 2050,
            "minimum": 2025,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ApplicantResponse": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "aadharNo": {
            "type": "string",
            "nullable": true
          },
          "course": {
            "type": "string",
            "nullable": true
          },
          "departmentId": {
            "type": "integer",
            "format": "int32"
          },
          "department": {
            "$ref": "#/components/schemas/Department"
          },
          "session": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Department": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "totalTeacher": {
            "type": "integer",
            "format": "int32"
          },
          "staffs": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Staff"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Position": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "Staff": {
        "required": [
          "dob",
          "name",
          "position"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "dob": {
            "type": "string",
            "format": "date"
          },
          "departmentId": {
            "type": "integer",
            "format": "int32"
          },
          "department": {
            "$ref": "#/components/schemas/Department"
          },
          "position": {
            "$ref": "#/components/schemas/Position"
          },
          "photoUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}